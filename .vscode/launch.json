  "version": "0.2.0",
  "configurations": [
    {
      "name": "ChurchWebHelper (local)",
      "type": "python",
      "request": "launch",
      "module": "church_web_helper",
      "cwd": "${workspaceFolder}",
      "env": {
        "FLASK_APP": "main_web.py",
        "FLASK_DEBUG": "1",
        "PYTHONUNBUFFERED": "1",
        "CT_DOMAIN": "https://elkw1610.krz.tools",
        "COMMUNI_SERVER": "https://api.communiapp.de/rest",
        "VERSION": "VS Code Launch config w/o version"
      },
      "args": [
        "run",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Docker Compose Up",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/.vscode/tasks.json",
      "preLaunchTask": "Docker Compose Up",
      "outFiles": []
    },
    {
      "name": "Docker Compose Debug Up",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/.vscode/tasks.json",
      "preLaunchTask": "Docker Compose Debug Up",
      "outFiles": []
    }
  ]
}